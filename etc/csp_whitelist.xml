<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="style-src">
            <values>
                <value id="adobeio_static_style" type="host">*.adobeio-static.net</value>
                <value id="adobecom_static_style" type="host">*.adobe.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="adobeio_static_js" type="host">*.adobeio-static.net</value>
                <value id="adobecom_static_js" type="host">*.adobe.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
